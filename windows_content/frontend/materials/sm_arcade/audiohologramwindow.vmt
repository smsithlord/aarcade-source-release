"Unlittwotexture"
{
	"$model" 1
	"$basetexture" "sm_arcade/audiostrip"
   "$additive" 1
   //"$selfillum" 1
   "$texture2" "dev/dev_scanline"
   "$surfaceprop" "glass"
   "%compileNoLight" 1
   "%compilepassbullets" 1

	$color2 "{ 255 255 255 }"

	"Proxies"
	{
      "TextureScroll"
      {
         "texturescrollvar" "$texture2transform"
         "texturescrollrate" .10
         "texturescrollangle" 270
      }
      
		"AAAudioPeak"
		{
			"audioUseAVR" "0"
			"audioPeakVar" "$color2"
			"audioPeakMin" "0.1"
			"audioPeakMax" "0.3"
			"audioPeakAmp" "1"
		}
	}
}